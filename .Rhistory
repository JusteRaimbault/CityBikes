radius=c();time=c();
for(i in config_numbers - 1){
for(j in 0:(maxtime-1)){
s = unlist(grid[seq(from=(i* 20 ) + 1,by=1,length.out=4),beginindex_mlf + (step_index * j)])
mlf = append(mlf,mean(s));
mlfse=append(mlfse,sd(s));
radius=append(radius,i*100)
time=append(time,j)
}
}
dat = data.frame(heterogeneity=mlf,mlfse,radius,time)
limitsmlf <- aes(ymax = heterogeneity + mlfse, ymin= heterogeneity - mlfse)
prent <- ggplot(dat, aes(colour=radius, y= heterogeneity, x= time))
prent + geom_line(aes(group=radius)) + geom_errorbar(limitsmlf, width=0.2)
}
